	<!-- This is the Assign block -->
	<form id="ECMAScript_AssignObj">
		<block>
			<log gvp:level="1">
				<value expr="'AppState.ObjCall.vActCode is '+AppState.ObjCall.vActCode;"/>
			</log>
    	</block>
		<block>
			<script>
				AppState.vActCode=AppState.ObjCall.vActCode;
			</script>	
			<goto next="#Branching_ActCodeWhatsapp" />
		</block>
	</form>

	<!-- This is the Subdialog block -->
	<form id="Sub_000101">

		<subdialog name="Sub_000101Subdialog" fetchhint="prefetch"  src="../src-gen/sub_000101.vxml">
    		<param name="varActionParentId" expr="'sub_WhatsappEnrollment'"/>
			<param name="ObjCall" expr="AppState.ObjCall" />
			<filled>
				<if cond="Sub_000101Subdialog.ObjCall != 'undefined'">
  					<assign name="AppState.ObjCall" expr="Sub_000101Subdialog.ObjCall" /> 
