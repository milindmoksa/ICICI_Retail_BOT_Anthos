				AppState.ObjCall.vServiceType='flexisucc';
AppState.ObjCall.vsubServiceType='success';

AppState.ObjCall.vTrav1=AppState.ObjCall.vTrav1+'2;';
				}
				catch(err){}
				
				</script>
			
				<goto next="#Assign13" />
			</if>
		</filled>
			
