					if (AppState.Looping_Count_inprogress == undefined || AppState.Looping_Count_inprogress == false) {
						AppState.vCount = 1;
						AppState.Looping_Count_inprogress = true;
					} else {
						++AppState.vCount;
						// check if at least one of the exit condition(s) is met
						if (!(AppState.vCount &lt;= 1)) {
							AppState.Looping_Count_inprogress = false;
						} else {
						}
					}
				} catch (e) {
					AppState.Looping_Count_inprogress = false;
				}
			</script>
			
  			<if cond="AppState.Looping_Count_inprogress == true">
				<!-- This is the NextLink -->
