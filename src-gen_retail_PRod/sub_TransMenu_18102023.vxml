				<if cond="SubAppStatusSubdialog.vTrav3 != 'undefined'">
  					<assign name="AppState.vTrav3" expr="SubAppStatusSubdialog.vTrav3" /> 
  				</if>
				<if cond="SubAppStatusSubdialog.vTrav4 != 'undefined'">
  					<assign name="AppState.vTrav4" expr="SubAppStatusSubdialog.vTrav4" /> 
