			<goto next="#ECMAScript_AssigntoObject" />
		</block>
	</form>

	<!-- This is the Menu block -->
	<form id="Copy_1_Menu_TS" gvp:cleardtmf="true">
        
        	        
        <script srcexpr="AppState.VOXFILEDIR + '/PromptSwitch.js'" />
        <script srcexpr="AppState.VOXFILEDIR + '/' + AppState.vLang + '/' + AppState.vLang + '.js'" />
        <block>
	        <script>
	            AppState.PREV_APP_LANGUAGE = AppState.APP_LANGUAGE;
	            AppState.PREV_APP_ASR_LANGUAGE = AppState.APP_ASR_LANGUAGE;
	            AppState.APP_LANGUAGE = AppState.vLang;
	            AppState.APP_ASR_LANGUAGE = AppState.vLang;
	        </script>
        </block>
        
		<property name="termchar" value="D"/>
		<property name="inputmodes" value="dtmf" />
		<property name="bargein" value="true" />
		<block name="Copy_1_Menu_TSP0">
  			<prompt gvp:langexpr="AppState.APP_LANGUAGE">
				<audio expr="AppState.VOXFILEDIR + '/' + AppState.APP_LANGUAGE + '/' + 'NPnewproductorapplicationstatus.wav'"></audio>
				<audio expr="AppState.VOXFILEDIR + '/' + AppState.APP_LANGUAGE + '/' + 'Titanium_Product_Menu.wav'"></audio>
				<audio expr="AppState.VOXFILEDIR + '/' + AppState.APP_LANGUAGE + '/' + 'Whatsapp4.wav'"></audio>
				<audio expr="AppState.VOXFILEDIR + '/' + AppState.APP_LANGUAGE + '/' + 'CBDC5_English.wav'"></audio>
    		</prompt>
		</block>
		
		<var name="retryCount" expr="1"/>
		<field name="Copy_1_Menu_TSOption">
			<property name="timeout" value="10s"/>
			<option dtmf="*" value="star" />
			<option dtmf="1" value="1" />
			<option dtmf="2" value="2" />
			<option dtmf="3" value="3" />
			<option dtmf="4" value="4" />
			<option dtmf="5" value="5" />
  		</field>
		<filled mode="all" namelist="Copy_1_Menu_TSOption">
		<assign name="AppState.vMenuInput" expr="Copy_1_Menu_TSOption" />
			<if cond="Copy_1_Menu_TSOption == 'star'">
	        <script>resetLanguage()</script>
            
				<goto next="#SubAppStatus" />
			</if>
			<if cond="Copy_1_Menu_TSOption == '1'">
	        <script>resetLanguage()</script>
            
				<!-- Post Action -->
				<script>try{
				AppState.ObjCall.IVR_ProductCode='dc';
AppState.ObjCall.IVR_AuthFlag='N'
AppState.ObjCall.vProductInfo='Debit'
AppState.ObjCall.vTrav1=AppState.ObjCall.vTrav1+'1;';
				}
				catch(err){}
				
				</script>
			
				<goto next="#Branching_EnrollFlag" />
			</if>
			<if cond="Copy_1_Menu_TSOption == '2'">
	        <script>resetLanguage()</script>
            
				<!-- Post Action -->
				<script>try{
				AppState.ObjCall.vTrav1=AppState.ObjCall.vTrav1+'2;';
				}
				catch(err){}
				
				</script>
			
				<goto next="#ECMAScript_IVR_CC" />
			</if>
			<if cond="Copy_1_Menu_TSOption == '3'">
	        <script>resetLanguage()</script>
            
				<!-- Post Action -->
				<script>try{
				AppState.ObjCall.vTrav1=AppState.ObjCall.vTrav1+'3;';
				}
				catch(err){}
				
				</script>
			
				<goto next="#Assign_LOANS" />
			</if>
			<if cond="Copy_1_Menu_TSOption == '4'">
	        <script>resetLanguage()</script>
            
				<!-- Post Action -->
				<script>try{
				AppState.ObjCall.vSelectService='4TS';
AppState.ObjCall.IVR_ProductCode='cc';
AppState.ObjCall.IVR_AuthFlag='N';
AppState.ObjCall.vProductInfo='';
AppState.ObjCall.vWhatsapp='WAPP';

//traversal
AppState.ObjCall.vTrav1 =AppState.ObjCall.vTrav1 + '8;';
				}
				catch(err){}
				
				</script>
			
				<goto next="#Assign_Whatsapp" />
			</if>
			<if cond="Copy_1_Menu_TSOption == '5'">
	        <script>resetLanguage()</script>
            
				<!-- Post Action -->
				<script>try{
				AppState.ObjCall.IVR_AuthFlag='N'
AppState.ObjCall.vTrav1=AppState.ObjCall.vTrav1+'5H;';
AppState.vSubCallFlag='AT';
AppState.ObjCall.vProductInfo='';
AppState.ObjCall.vCBDCFlag='Y';
				}
				catch(err){}
				
				</script>
			
				<goto next="#ECMAScript1" />
			</if>
		</filled>
			
		<catch event="noinput" cond="retryCount == 1">
		<assign name="retryCount" expr="retryCount + 1"/>
		<prompt bargein="true" gvp:langexpr="AppState.APP_LANGUAGE">
				<audio expr="AppState.VOXFILEDIR + '/' + AppState.APP_LANGUAGE + '/' + 'COR.NOINPUT.wav'"></audio>
  		</prompt>
  		<clear namelist="Copy_1_Menu_TSP0"/>
  		</catch>
			
		<catch event="nomatch" cond="retryCount == 1">
		<assign name="retryCount" expr="retryCount + 1"/>
		<prompt bargein="true" gvp:langexpr="AppState.APP_LANGUAGE">
				<audio expr="AppState.VOXFILEDIR + '/' + AppState.APP_LANGUAGE + '/' + 'COR.INVALID.wav'"></audio>
  		</prompt>
  		<clear namelist="Copy_1_Menu_TSP0"/>
  		</catch>
		<catch event="noinput"  cond="retryCount==2">
	        <script>resetLanguage()</script>
	  		<throw event="com.genesyslab.composer.toomanynoinputs" message="exceeded the number of noinput retries" />
	  	</catch>
		<catch event="nomatch"  cond="retryCount==2">
	        <script>resetLanguage()</script>
	  		<throw event="com.genesyslab.composer.toomanynomatches" message="exceeded the number of nomatch retries" />
	  	</catch>
		<catch event="com.genesyslab.composer.toomanynoinputs">
	        <script>resetLanguage()</script>
			<goto next="#Assign_NoInput" />
    	</catch>
		<catch event="com.genesyslab.composer.toomanynomatches">
	        <script>resetLanguage()</script>
			<goto next="#Assign_NoMatch" />
    	</catch>
	</form>

	<!-- This is the Branching block -->
	<form id="Branching_SelectService">
		<block>
			<if cond="(AppState.ObjCall.vSelectSrvTime=='true' &amp;&amp;AppState.ObjCall.vSubCallFlag=='LOC')">
				
				<!-- This is the OutLink -->
				<goto next="#Assign_LOC" />
			</if>
