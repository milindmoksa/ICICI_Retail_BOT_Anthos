	<form id="ECMAScript_IVR_CC">
		<block>
			<script>
				if(AppState.ObjCall.botflag =='true' || AppState.ObjCall.botflag == 'TRUE'){
					AppState.vIVR_MM ='IVR_MM'
					AppState.ObjCall.vIVR_MM =AppState.vIVR_MM
					__Log('BOT Condition True Manipulation vIVR_MM :  '+AppState.ObjCall.vIVR_MM )
					}
