			<if cond="AppState.ObjCall.vCustseg.search('TS')!=-1 || AppState.ObjCall.vCustseg.search('WS')!=-1">
				
				<!-- Post Action -->
				<script>try{
				AppState.vSubCallFlag='AT';

AppState.ObjCall.vServiceType='agenttransfer';
AppState.ObjCall.vsubServiceType='success';
				}
				catch(err){}
				
				</script>
				
				<!-- This is the OutLink -->
				<goto next="#ECMAScript_ReturnObj" />
			</if>
			<if cond="AppState.ObjCall.vSubCallFlag=='DISCONNECT' || AppState.vSubCallFlag='DISCONNECT'">
				
				<!-- Post Action -->
				<script>try{
				AppState.vSubCallFlag='DISCONNECT'
				}
				catch(err){}
				
				</script>
				
				<!-- This is the OutLink -->
				<goto next="#ECMAScript_ReturnObj" />
			</if>
			<goto next="#Subdialog_CreditCardBalance" />
		</block>
	</form>

	<!-- This is the Assign block -->
	<form id="ECMAScript_AssignObjCC_BOT">
		<block>
			<log gvp:level="1">
				<value expr="'AppState.vLang is '+AppState.vLang"/>
			</log>
			<log gvp:level="1">
				<value expr="'AppState.ObjCall.vLang is '+AppState.ObjCall.vLang"/>
			</log>
			<log gvp:level="1">
				<value expr="'AppState.vANI is '+AppState.vANI"/>
			</log>
    	</block>
		<block>
			<script>
				if(AppState.ObjCall.vTempSubcallflag=='DISCONNECT'){
					AppState.ObjCall.vSubCallFlag='DISCONNECT';
					__Log('AppState.ObjCall.vSubCallFlag outside CCMAIN is :   '+AppState.ObjCall.vSubCallFlag);
					}
					AppState.vSubCallFlag=AppState.ObjCall.vSubCallFlag;
			</script>	
			<goto next="#Branching_TS_WS" />
		</block>
	</form>

	<!-- This is the Branching block -->
	<form id="Branching_BOT_CCStmt_PING">
		<block>
			<if cond="AppState.ObjCall.vCCStmt =='CCStmt' || AppState.ObjCall.vBotNode=='IVR_NODE_2_2_en' || AppState.ObjCall.vBotNode== 'IVR_NODE_2_2_hi' || AppState.ObjCall.vBotNode=='IVR_NODE_2_2_DURATION_SPECIFIED_en' || AppState.ObjCall.vBotNode== 'IVR_NODE_2_2_DURATION_SPECIFIED_hi'">
				
				<!-- This is the OutLink -->
				<goto next="#Subdialog_CreditCardTransMenu" />
			</if>
			<goto next="#Branching_AMEX1" />
		</block>
	</form>

	<!-- This is the Branching block -->
	<form id="Branching_Customer_Disconnect">
		<block>
			<if cond="AppState.ObjCall.CallDisconnectFlag=='D'">
				
				<!-- This is the OutLink -->
				<goto next="#DB_Validation" />
			</if>
			<goto next="#Exit_CreditCardMain" />
		</block>
	</form>

	<!-- This is the Log block -->
	<form id="DB_Validation">
		<block>
			<log gvp:level="1">
				<value expr="'****ValueofTrav1 is**** ' + AppState.vTrav1"/>
			</log>
			<log gvp:level="1">
				<value expr="'AppState.ObjCall.vTxnStTime is : '+AppState.ObjCall.vTxnStTime"/>
			</log>
    	</block>
    	<block>
    		<goto next="#Subdialog_InsertMasterData" />
    	</block>
	</form>

	<!-- This is the Subdialog block -->
	<form id="Subdialog_InsertMasterData">

		<subdialog name="Subdialog_InsertMasterDataSubdialog" fetchhint="prefetch"  src="../src-gen/sub_InsertMasterData.vxml">
    		<param name="varActionParentId" expr="'sub_CreditCardMain'"/>
			<param name="ObjCall" expr="AppState.ObjCall" />
			<filled>
				<if cond="Subdialog_InsertMasterDataSubdialog.ObjCall != 'undefined'">
  					<assign name="AppState.ObjCall" expr="Subdialog_InsertMasterDataSubdialog.ObjCall" /> 
  				</if>
				<goto next="#Disconnect_error" />
			</filled>
		</subdialog>
		
    	<catch event="error.semantic">
    <goto next="#Disconnect_error" />
    	</catch>
    	<catch event="error.badfetch">
    <goto next="#Disconnect_error" />
    	</catch>
    	<catch event="error">
    <goto next="#Disconnect_error" />
    	</catch>
	</form>

	<!-- This is the Disconnect block -->
	<form id="Disconnect_error">
		<block>

			<disconnect />
    	</block>
	</form>

	<!-- This is the Assign block -->
	<form id="ECMAScript_getCallDisconnect">
		<block>
			<script>
				try {
					
					__Log('******** Call Disconnect Method Call AppState.ObjCall.vCallEndReason : '+AppState.ObjCall.vCallEndReason+' AppState.ObjCall.vSubCallFlag : '+AppState.ObjCall.vSubCallFlag);
					AppState.ObjCall.CallDisconnectFlag=getCallDisconnect(AppState.ObjCall.vCallEndReason,AppState.ObjCall.vSubCallFlag);
					__Log('******* AppState.ObjCall.CallDisconnectFlag : '+AppState.ObjCall.CallDisconnectFlag);
					
					} catch (error) {
					__Log('********* Error while get Calldisconnect Flag');
					}
			</script>	
			<goto next="#Branching_Customer_Disconnect" />
		</block>
	</form>

	<!-- This is the Assign_Lang block in Disabled mode -->
	<form id="Assign_Lang">
    	<block>
    		<goto next="#SetLanguage" />
    	</block>
	</form>

	<!-- This is the Prompt_PBlock block in Disabled mode -->
	<form id="Prompt_PBlock">
    	<block>
    		<goto next="#ECMAScript_ReturnObj" />
    	</block>
	</form>

	<!-- This is the Prompt_Blocked block in Disabled mode -->
	<form id="Prompt_Blocked">
    	<block>
    		<goto next="#ECMAScript_ReturnObj" />
    	</block>
	</form>

	<!-- This is the BOT_IVR block in Disabled mode -->
	<form id="BOT_IVR">
    	<block>	
    		<goto next="#ECMAScript_AT_" />
    	</block>
	</form>

	<!-- This is the ECMAScript_AT_ block in Disabled mode -->
	<form id="ECMAScript_AT_">
    	<block>
    		<goto next="#ECMAScript_AssignObjCCBal" />
    	</block>
	</form>

</vxml>
