		<block>
			<script>
				try {
					// if not initialized, initialize the counter/iterator
					if (AppState.Copy_1_Looping_InvalidCard_inprogress == undefined || AppState.Copy_1_Looping_InvalidCard_inprogress == false) {
						AppState.vCounter = 1;
						AppState.Copy_1_Looping_InvalidCard_inprogress = true;
					} else {
						++AppState.vCounter;
						// check if at least one of the exit condition(s) is met
						if (!(AppState.vCounter &lt;= 1) || (AppState.vSubCallFlag='DISCONNECT')) {
							AppState.Copy_1_Looping_InvalidCard_inprogress = false;
						} else {
						}
					}
				} catch (e) {
					AppState.Copy_1_Looping_InvalidCard_inprogress = false;
				}
			</script>
			
  			<if cond="AppState.Copy_1_Looping_InvalidCard_inprogress == true">
