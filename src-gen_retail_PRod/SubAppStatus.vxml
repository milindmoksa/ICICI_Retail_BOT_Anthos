	<form id="Assign_AT_Loans">
		<block>
			
			<assign name="AppState.vSubCallFlow" expr="'AT'" />
			<assign name="AppState.IVR_Skill" expr="AppState.ObjCall.IVR_Skill=AppState.IVR_Skill;" />
			<assign name="AppState.vTrav1" expr="AppState.vTrav1+''+AppState.ObjCall.IVR_ProductCode" />				
			<goto next="#ECMAScript_RoutingLoan" />
		</block>
	</form>

	<!-- This is the Assign block -->
	<form id="Assign_AT_CCards">
		<block>
			
			<assign name="AppState.vSubCallFlow" expr="'AT'" />
			<assign name="AppState.IVR_Skill" expr="AppState.IVR_Skill = '665_CreditCard';" />
			<assign name="AppState.vTrav1" expr="AppState.vTrav1+''+AppState.ObjCall.vProduct" />				
			<goto next="#ECMAScript_RoutingCCard" />
		</block>
	</form>

	<!-- This is the Assign block -->
	<form id="ECMAScript_RoutingSav">
		<block>
			<script>
				AppState.ObjCall.vSubCallFlag=AppState.vSubCallFlow;
					AppState.ObjCall.o_TransferDestination ='65630';
					AppState.ObjCall.vProduct=AppState.vProduct;
					AppState.ObjCall.IVR_ProductCode='SAC';
			</script>	
			<goto next="#ECMAScript_ReturnObj" />
		</block>
	</form>

	<!-- This is the Assign block -->
	<form id="ECMAScript_RoutingLoan">
		<block>
			<script>
				AppState.ObjCall.vSubCallFlag=AppState.vSubCallFlow;
					AppState.ObjCall.o_TransferDestination =AppState.o_TransferDestination;
					AppState.IVR_ProductCode=AppState.ObjCall.IVR_ProductCode;
					AppState.vProduct=AppState.ObjCall.vProduct;
			</script>	
			<goto next="#ECMAScript_ReturnObj" />
		</block>
	</form>

	<!-- This is the Assign block -->
	<form id="ECMAScript_RoutingCCard">
		<block>
			<script>
				AppState.ObjCall.vSubCallFlag=AppState.vSubCallFlow;
					AppState.ObjCall.o_TransferDestination ='65632';
					AppState.ObjCall.IVR_ProductCode='CC';
					AppState.ObjCall.vCCFlag='Y';
					AppState.vProduct=AppState.ObjCall.vProduct;
			</script>	
			<goto next="#ECMAScript_ReturnObj" />
		</block>
	</form>

	<!-- This is the Assign block -->
	<form id="Copy_1_Assign_SCF">
		<block>
			<script>
				AppState.ObjCall.vNPMenuOpt='7'
					AppState.ObjCall.vProduct='SCF';
					AppState.ObjCall.IVR_ProductCode='SCF';
					AppState.ObjCall.vSCF=='true';
					AppState.ObjCall.vChannel=='IVR';
			</script>	
			<goto next="#Assign_ANILength" />
		</block>
	</form>

	<!-- This is the Assign block -->
	<form id="Assign_SCF">
		<block>
			<script>
				AppState.vTrav1=AppState.vTrav1;
					AppState.ObjCall.vProduct='SCF';
					AppState.ObjCall.IVR_ProductCode='SCF';
					AppState.ObjCall.vSCF=='true';
					AppState.ObjCall.vChannel=='IVR';
			</script>	
			<goto next="#Subdialog_PuchCustomerCLI_SCF" />
		</block>
	</form>

	<!-- This is the Subdialog block -->
	<form id="Subdialog_PuchCustomerCLI_SCF">

		<subdialog name="Subdialog_PuchCustomerCLI_SCFSubdialog" fetchhint="prefetch"  src="../src-gen/sub_WS_PushCustInCLISCF.vxml">
    		<param name="varActionParentId" expr="'SubAppStatus'"/>
			<param name="vInputPhoneNum" expr="AppState.vInputPhoneNum" />
			<param name="ObjCall" expr="AppState.ObjCall" />
			<filled>
				<if cond="Subdialog_PuchCustomerCLI_SCFSubdialog.ObjCall != 'undefined'">
  					<assign name="AppState.ObjCall" expr="Subdialog_PuchCustomerCLI_SCFSubdialog.ObjCall" /> 
  				</if>
				<goto next="#Branching_CHK_Suc" />
			</filled>
		</subdialog>
		
	</form>

	<!-- This is the Assign block -->
	<form id="Copy_1_Assign_WW_SavLoanCredit">
