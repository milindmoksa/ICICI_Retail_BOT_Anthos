	<!-- This is the Looping block -->
	<form id="Copy_1_LoopingCount_">
		<block>
			<script>
				try {
					// if not initialized, initialize the counter/iterator
					if (AppState.Copy_1_LoopingCount__inprogress == undefined || AppState.Copy_1_LoopingCount__inprogress == false) {
						AppState.vCount = 1;
						AppState.Copy_1_LoopingCount__inprogress = true;
					} else {
						++AppState.vCount;
						// check if at least one of the exit condition(s) is met
						if (!(AppState.vCount &lt;= 1)) {
							AppState.Copy_1_LoopingCount__inprogress = false;
						} else {
						}
					}
				} catch (e) {
					AppState.Copy_1_LoopingCount__inprogress = false;
				}
			</script>
			
  			<if cond="AppState.Copy_1_LoopingCount__inprogress == true">
				<!-- This is the NextLink -->
				<goto next="#Log1" />
			<else/>
				<!-- This is the ExitLink -->
				<goto next="#WebService_INSERT" />
			</if>
		</block>
	</form>

